<UserControl x:Class="Winleafs.Wpf.Views.MainWindows.MostUsedEffectUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Winleafs.Wpf.Views.MainWindows"
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="180"
             MouseDoubleClick="UserControl_MouseDoubleClick"
             MouseEnter="UserControl_MouseEnter"
             MouseLeave="UserControl_MouseLeave">
    <Grid>
        <TextBlock x:Name="NameLabel" TextWrapping="Wrap" Foreground="White" FontSize="13"/>
    </Grid>
</UserControl>
